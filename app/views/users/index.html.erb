<div class="container">
<dev class="row">
<dev class="col-xs-3">
<%= render 'books/list', book: @book, user: @user %>
</dev>
<div class="col-xs-9">
<h3>Users</h3>
<table class="table table-striped">
  <thead>
  	<tr>
  		<th>image</th>
  		<th>name</th>
  		<th></th>
  	</tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
  	<tr>
  		<td>
  			<%= attachment_image_tag user, :profile_image, :fill, 50, 50, fallback: "no-image1.jpg", class: "img-circle pull-left profile-thumd" %>
  		</td>
  		<td><%= user.name %></td>
  		<td><%= link_to "Show", user_path(user.id) %></td>
  	</tr>
  	<% end %>
  </tbody>
</table>
</div>
</dev>
</div>